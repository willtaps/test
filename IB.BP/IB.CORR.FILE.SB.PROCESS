    SUBROUTINE IB.CORR.FILE.SB.PROCESS
    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT IB.BP I_F.IB.CORR.FILE.SB

    IF R.NEW(IB.CORR.RETRY) = "YES" THEN
        YR.FILE.ID = R.NEW(IB.CORR.TXN.REF)
        IF YR.FILE.ID THEN
            CMD = "COPY FROM IB.CLEARING.ARC TO IB.CLEARING " : YR.FILE.ID
            EXEC CMD
        END
    END
    R.NEW(IB.CORR.RETRY) = ""

    RETURN

END
