            ETEXT = 'NAO TEM SALDO SUFICIENTE. VALOR A PAGAR ':CHRG.AMT:YR.MOEDA
            CALL STORE.END.ERROR
        END ELSE
            CALL F.READ('F.ACCOUNT',DR.ACCT,R.ACCT,F.ACCOUNT,YERR)
            CALL F.READ('F.CARD.CHARGE','P24',R.CCHRG,F.CARD.CHARGE,ER)
