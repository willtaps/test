    PROGRAM RUN.UPD.CARD.COMP.SB

    HUSH ON
    EXECUTE 'SSELECT F.COMPANY WITH CONSOLIDATION.MARK EQ "N"'
    HUSH OFF

    V.COUNT = @SELECTED
    READLIST SEL.LIST ELSE NULL
    FOR X = 1 TO V.COUNT
        ID.COMPANY = SEL.LIST<X>

        CALL LOAD.COMPANY( ID.COMPANY )
        CALL B.ATM.CARD.COMPANY.SB
    NEXT X

    STOP

END
