SUBROUTINE IB.TAG.132.SB
*
    $INCLUDE I_COMMON
    $INCLUDE I_EQUATE
    $INSERT I_IB.TAG.SB.COMMON
    $INSERT I_F.IB.ATM.BALANCE.SB
*

    YR.OUT.ACCT = YR.RETURN
    YR.IN.ACCT = ""
    CALL IB.FMT.ACCOUNT.SB (YR.IN.ACCT , YR.OUT.ACCT)
    YR.RETURN = TRIM(YR.IN.ACCT)

    IF YR.RETURN[1,3] = "000" THEN
        YR.RETURN = "MZN" : YR.RETURN[9]
    END

    RETURN

END
